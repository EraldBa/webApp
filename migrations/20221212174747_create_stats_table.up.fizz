create_table("stats") {
  t.Column("id", "integer", {primary: true})
  t.Column("date", "datetime", {})
  t.Column("breakfast", "real", {"default": 0})
  t.Column("lunch", "real", {"default": 0})
  t.Column("dinner", "real", {"default": 0})
  t.Column("snacks", "real", {"default": 0})
  t.Column("protein", "real", {"default": 0})
  t.Column("carbs", "real", {"default": 0})
  t.Column("fats", "real", {"default": 0})
  t.Column("user_id", "int", {})
}